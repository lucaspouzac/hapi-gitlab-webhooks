{
  "name": "hapi-gitlab-webhooks",
  "version": "1.0.0",
  "description": "A Hapi plugin for receiving requests from the GitLab webhooks API.",
  "keywords": [
    "hapi",
    "gitlab",
    "api",
    "webhooks"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lpouzac/hapi-gitlab-webhooks.git"
  },
  "scripts": {
    "compile": "babel --presets es2015 -d dist/ lib/",
    "prepublish": "npm run compile",
    "start": "node example/server.js",
    "test": "mocha --reporter spec -r chai"
  },
  "authors": [
    "Lucas Pouzac <lucas.pouzac.pro@gmail.com> (https://github.com/lucaspouzac)"
  ],
  "license": "MIT",
  "dependencies": {
    "boom": "3.2.1",
    "joi": "8.4.0"
  },
  "peerDependencies": {
    "hapi": ">=10.x.x"
  },
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "chai": "3.5.0",
    "hapi": "13.x.x",
    "mocha": "2.5.3"
  },
  "bugs": {
    "url": "https://github.com/lpouzac/hapi-gitlab-webhooks/issues"
  },
  "homepage": "https://github.com/lpouzac/hapi-gitlab-webhooks#readme",
  "directories": {
    "example": "example",
    "lib": "lib",
    "test": "test"
  },
  "author": "Lucas Pouzac <lucas.pouzac.pro@gmail.com> (https://github.com/lucaspouzac)"
}
